{
  "name": "postgress",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prestart": "babel ./src --out-dir build",
    "start": "node ./src/bin/www",
    "dev": "nodemon --exec babel-node ./src/bin/www",
    "test": "nyc --reporter=html --reporter=text --reporter=lcov mocha --timeout 10000 -r @babel/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "runQuery": "babel-node ./src/utils/runQuery.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "~1.4.4",
    "coverage": "^0.4.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http": "0.0.1-security",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.3.0"
  },
  "lint": "./node_modules/.bin/eslint ./src",
  "prettier": "prettier --write '**/*.{js,json}' '!node_modules/**'",
  "postpretty": "npm run lint -- --fix",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.11.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.1.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2"
  }
}
